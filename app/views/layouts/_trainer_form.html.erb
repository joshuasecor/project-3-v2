
<%= form_for find_trainer, :url => { :controller => "/dashboard/trainers", :action => "create" } do |f| %>
	First Name: <%= f.text_field :first_name %><br><br>
	Last Name: <%= f.text_field :last_name %><br><br>
	Bio: <%= f.text_area :bio %><br><br>
	Featured? <%= f.check_box :is_featured %><br><br>
	<%= f.submit "Save", class: "btn btn-primary button" %>
<% end %>